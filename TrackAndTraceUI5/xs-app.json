{
  "welcomeFile": "/TrackAndTraceUI5/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "/v2/(.*)$",
      "target": "/v2/$1",
      "destination": "TrackAndTraceAPI_api"
    },
    {
      "source": "^/TrackAndTraceUI5/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}